#include <iostream>
#include <vector>
using namespace std;
int main()
{
    vector<int>array={10,11,2,4,3,12};
    cout<<"In this we are using Bubble sort.";
    int n= array.size();
    for (int i=n-1;i>=0;i--)
    {
        for (int j=0;j<n;j++)
        {
            if (array[j]>array[j+1])
            {
                swap(array[j],array[j+1]);
            }
        }
    }
    cout<<"This is your sorted array"<<"\n";
    for (int i=0;i<n;i++)
    {
        cout<<array[i]<<" ";
    }
}
